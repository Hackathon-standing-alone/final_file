<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.standing_alone">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WritePostActivity">
        </activity>
        <activity android:name=".ViewPostActivity">
        </activity>
        <activity android:name=".BorrowActivity">
        </activity>

        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="bf341tlltu" />
        <activity android:name=".HomeActivity"/>
        <activity android:name=".TaxiActivity"/>
        <activity android:name=".MapActivity"/>
        <activity android:name=".ButtonActivity"/>
        <activity android:name=".LoginActivity"/>
        <activity android:name=".PasswordResetActivity"/>
        <activity android:name=".SignUpActivity"/>

    </application>

</manifest>
